<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mushroom.trial">

    <application
        android:allowBackup="true"
        android:icon="@drawable/prono"
        android:label="@string/app_name"
        android:roundIcon="@drawable/prono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".utils.PqList"></activity>
        <activity android:name=".utils.OnboardActivity" />
        <activity android:name=".questionaires.ResultScreen" />
        <activity android:name=".questionaires.PQActivity" />
        <activity android:name=".utils.landPage" />
        <activity
            android:name=".utils.LoginSignupActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".utils.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>